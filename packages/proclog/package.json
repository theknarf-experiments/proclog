{
  "name": "proclog",
  "version": "0.1.0",
  "description": "ProcLog logic programming language for JavaScript/TypeScript (WASM)",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "wasm"
  ],
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest",
    "build:wasm": "wasm-pack build --target nodejs ../../crates/proclog-wasm --out-dir ../../packages/proclog/wasm",
    "build:ts": "tsc",
    "build": "npm run build:wasm && npm run build:ts",
    "clean": "rm -rf dist wasm node_modules"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "dependencies": {}
}
